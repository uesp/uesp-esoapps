            <OnUpdate>
                uespLog.UpdateCoordinates()
            </OnUpdate>

            <Controls>
                <Label name="$(parent)Value" font="ZoFontGameLargeBold" color="CFDCBD" wrapMode="ELLIPSIS" verticalAlignment="CENTER" text="">
                    <AnchorFill />
                </Label>
            </Controls>
        </TopLevelControl>
	</Controls>
</GuiXml>